			<div class="well">
				<% menu.forEach(function(mi) { %>
					<h5><%- mi.title %></h5>
					<ul>
					<% mi.menu.forEach(function(mii) { %>
						<li><a href="<%- mii.href %>" class="<%- mii.content ? 'content' : '' %>"><%- mii.name %></a></li> 				
					<% }); %>
					</ul>
				<% }); %>
			</div>