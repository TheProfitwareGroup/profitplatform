<p><b>Документы:</b></p>
<%- docsScroller %>
<% if(ref != '') { %>
    <form class="form-horizontal" id="docs_new" method="GET" action="/docs_push">
        <div class="form-actions">
            <button type="submit" class="btn btn-primary">Провести документ</button>
        </div>
    </form>
    <p><b>Содержимое документа:</b></p>
    <%- documentScroller %>
    <p><b>Движения документа:</b></p>
    <%- historyScroller %>
<% } %>