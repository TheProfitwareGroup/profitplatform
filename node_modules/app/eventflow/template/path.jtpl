<ul class="breadcrumb">
    <% if(path) {
        for(i = 0; i < Object.keys(path).length; i++) { %>
    <li class="<%- (i == Object.keys(path).length - 1) ? 'active' : ''%>">
        <% if(path[i].href) { %>
            <a href="<%- path[i].href %>"><%- path[i].title %></a>
        <% } else { %>
            <%- path[i].title %>
        <% } %>
        <% if(i != Object.keys(path).length - 1) { %>
            <span class="divider">&gt;</span>
        <% } %>
    </li>
    <% } } %>
</ul>