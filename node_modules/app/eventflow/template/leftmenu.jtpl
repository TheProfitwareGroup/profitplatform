                <ul class="nav nav-list">
				<% menu.forEach(function(mi) { %>
					<li class="nav-header"><%- mi.title %></li>
					<% mi.menu.forEach(function(mii) { %>
						<li class="<%- mii.active ? 'active' : '' %>">
						  <a href="<%- mii.href %>">
						      <% if(mii.icon) { %>
						          <i class="icon-<%- mii.icon %>"></i>
						      <% } %>
						      <%- mii.name %>
						   </a>
						</li> 				
					<% }); %>
				<% }); %>
				</ul>