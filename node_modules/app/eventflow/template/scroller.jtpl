<table class="table table-bordered table-striped">
    <thead>
        <% table.head.forEach(function(column) { %>
            <td><%- column.title %></td>
        <% }); %>
    </thead>
    <tbody>
        <% table.data.forEach(function(row) { %>
            <tr>
                <% table.head.forEach(function(column) { %>
                    <% if(typeof table.rowspan == 'number') { %>
                        <% if(typeof column.name == 'object') { %>
                            <% for(var i = 0 ; i < column.name.length - 1 ; i += 1) { %>
                                <%- i != 0 ? '<tr>' : '' %><td><% if(typeof column.name[i].name == 'function') { %>
                                    <%- column.name[i].name(row) %>
                                <% } else { %>
                                    <%- row[column.name[i].name] %>
                                <% } %></td><%- i != 0 ? '</tr>' : '' %>
                            <% } %>
                            <tr><td rowspan="<%- table.rowspan - column.name.length + 1 %>">
                            <% if(typeof column.name[column.name.length - 1].name == 'function') { %>
                                <%- column.name[column.name.length - 1].name(row) %>
                            <% } else { %>
                                <%- row[column.name[column.name.length - 1].name] %>
                            <% } %></td></tr>
                        <% } else { %>
                            <td rowspan="<%- table.rowspan %>"><% if(typeof column.name == 'function') { %>
                                <%- column.name(row) %>
                            <% } else { %>
                                <%- row[column.name] %>
                            <% } %></td>
                        <% } %>
                    <% } else { %>
                        <td><% if(typeof column.name == 'function') { %>
                            <%- column.name(row) %>
                        <% } else { %>
                            <%- row[column.name] %>
                        <% } %></td>
                    <% } %>
                <% }); %>
            </tr>
         <% }); %>
    </tbody>
</table>